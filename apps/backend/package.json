{
    "private": true,
    "scripts": {
        "build:css": "tailwindcss build src/input.css -o public/styles.css",
        "lint": "npx @biomejs/biome lint --error-on-warnings",
        "lint-fix": "npx @biomejs/biome lint --write",
        "lint-fix-unsafe": "npx @biomejs/biome lint --write --unsafe",
        "format": "npx @biomejs/biome format --write",
        "dev": "node --no-warnings --enable-source-maps --env-file=.env --loader ts-node/esm src/index.ts",
        "build": "tsc -p tsconfig.json",
        "start": "npm ci && tsc -p tsconfig.json && NODE_ENV=production node --env-file=.env ./dist/index.js"
    },
    "type": "module",
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "axios": "^1.7.2",
        "fastify": "^4.28.0",
        "https-proxy-agent": "^7.0.4",
        "lodash": "^4.17.21",
        "pino": "^8.21.0",
        "pino-pretty": "^10.3.1",
        "poe-trade-fetch": "^1.8.12",
        "uuid": "^9.0.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.8.2",
        "@types/async-lock": "^1.4.2",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.5",
        "@types/uuid": "^9.0.8",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.2"
    }
}

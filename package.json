	"scripts": {
		"prebuild": "rimraf dist",
		"build": "yarn prebuild && yarn build:next && yarn build:nest",
		"build:next": "next build",
		"build:nest": "nest build --path ./tsconfig.server.json",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "cross-env NODE_ENV=production node ./dist/server/main.js",
		"start:next": "next dev",
		"start:dev": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch --path tsconfig.server.json",
		"start:debug": "nest start --path ./tsconfig.server.json --debug --watch",
		"start:docker": "docker run --rm -p 3000:3000 -p 7655:22 -it $(docker build --build-arg ssh_pub_key=\"$(cat ~/.ssh/id_rsa.pub)\" -q .)",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "cross-env NODE_ENV=test jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage"
	},
